  },
  "devDependencies": {
    "@salesforce/dev-config": "^2.1.3",
    "@salesforce/dev-scripts": "^1.0.2",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.2.3",
