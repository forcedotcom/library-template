    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^7.0.4",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "sinon": "10.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
